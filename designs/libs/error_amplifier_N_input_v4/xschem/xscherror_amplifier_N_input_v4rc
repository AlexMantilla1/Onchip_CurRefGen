# Source the PDK xscherror_amplifier_N_input_v4rc file
if {![info exists PDK]} {
    source $env(PDK_ROOT)/$env(PDK)/libs.tech/xscherror_amplifier_N_input_v4/xscherror_amplifier_N_input_v4rc
}

# Add current directory to xscherror_amplifier_N_input_v4 library path
append XSCHEM_LIBRARY_PATH :[file dirname [info script]] 
